git status //ves los archivos que se modificaron y cuales estan agregados al stash para commitear.
git add <archivo> //git add . agrega todos los archivos al stash
git status //para comprobar los archivos agregados.
git commit -m "<mensaje>"
git push origin <branch>
git checkout master // para posicionarte en la ramas master.
git pull
git merge <branch>


git checkout -b <name_branch> //sirve para crear una rama nueva en el repositorio local con el nombre <name_branch>